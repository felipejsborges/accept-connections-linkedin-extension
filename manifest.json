{
  "manifest_version": 2,
  "name": "Aceitar Conexões | LinkedIn",
  "version": "1.0",
  "description": "Aceitar Múltiplas Conexões no LinkedIn",
  "author": "Felipe Borges",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
